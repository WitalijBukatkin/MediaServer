<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox alignment="TOP_CENTER" prefHeight="438.0" prefWidth="345.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.mediaserver.clients.msdesktop.presentation.auth.register.RegisterPresenter">
    <children>
        <Label contentDisplay="CENTER" text="Registration">
            <font>
                <Font name="Helvetica" size="51.0" />
            </font>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets bottom="50.0" top="50.0" />
            </padding>
        </Label>
        <VBox alignment="CENTER" spacing="10.0">
            <children>
                <HBox alignment="CENTER" spacing="10.0">
                    <children>
                        <Label text="Login">
                            <padding>
                                <Insets right="60.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="login" style="-fx-background-color: rgba(128, 67, 80, 0.31)rgba(128, 67, 80, 0.31);" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label text="Password">
                     <padding>
                        <Insets right="35.0" />
                     </padding></Label>
                  <PasswordField fx:id="password" style="-fx-background-color: rgba(128, 67, 80, 0.31)rgba(128, 67, 80, 0.31);" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" spacing="10.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="95.0" text="Repeat">
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                        </Label>
                        <PasswordField fx:id="passwordRepeat" style="-fx-background-color: rgba(128, 67, 80, 0.31)rgba(128, 67, 80, 0.31);" />
                    </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
                </HBox>
                <HBox alignment="CENTER">
                    <VBox.margin>
                        <Insets top="30.0" />
                    </VBox.margin>
                    <children>
                        <Button onAction="#register" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="272.0" style="-fx-background-color: rgba(128, 67, 80, 0.31)rgba(128, 67, 80, 0.31);" text="Register">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</VBox>
